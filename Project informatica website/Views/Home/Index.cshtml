@using Project_informatica_website.Dbase;
@model List<Actor>
@{
    ViewData["Title"] = "Home Page";
}
<div class="text-center">
    <h1 class="display-4">Welcome</h1>


</div>
<div class="box">
    @if (Model != null)
    {
        @foreach (Actor m in Model)
        {
            <p style="color:white">@m.Actor_ID - @m.Name @m.Birth_Date-</p>

            <img src="~/img/@m.File"
                 class="Border"
                 height="259" width="157"
                 asp-append-version="true" />
        }
    }

</div>